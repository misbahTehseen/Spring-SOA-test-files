<aura:component >
    <aura:handler name="init" action="{!c.RecentRecords}" value="{!this}" />
    <aura:attribute name="accounts" type="Account[]" />
    <ul>
        <aura:iteration items="{!v.accounts}" var="acc" >
            <li Class="myList" ><h1>{!acc.name} </h1></li>
        </aura:iteration>
    </ul>
</aura:component>
